{
  "documentationUrl": "https://docs.airbyte.com/integrations/destinations/veeva-vault",
  "supported_destination_sync_modes": [
    "overwrite", "append", "append_dedup"
  ],
  "supportsIncremental": true,
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Destination Veeva Vault",
    "type": "object",
    "required": ["vaultDNS", "username", "password"],
    "additionalProperties": false,
    "properties": {
      "vaultDNS": {
        "type": "string",
        "description": "Veeva Vault DNS"
      },
      "username": {
        "type": "string",
        "description": "Provide Veeva Vault username"
      },
      "password": {
        "type": "string",
        "description": "Profive Veeva Vault password"
      },
      "api_version": {
        "type": "string",
        "description": "Veeva Vault API Version specification - currently supports v23.2. See > https://developer.veevavault.com/docs/#versioning "
      }
    }
  }
}
